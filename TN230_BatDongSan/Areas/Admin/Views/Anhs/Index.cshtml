@model IEnumerable<DbContextModel.Framework.Anh>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>STT</th>
        <th>
            chỗ này có sai
            @Html.DisplayNameFor(model => model.ThongTinBDS.MaTin)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DuongDan)
        </th>
        <th></th>
    </tr>

@{
    var i = 0;
}
@foreach (var item in Model) {
    <tr>
        <td>
            @i
            @{ i++; }
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ThongTinBDS.TieuDe)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DuongDan)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.MaAnh }) |
            @Html.ActionLink("Details", "Details", new { id=item.MaAnh }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.MaAnh })
        </td>
    </tr>
}

</table>
